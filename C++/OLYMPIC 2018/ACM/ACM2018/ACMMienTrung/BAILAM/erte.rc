#include <iostream>
#include <string.h>
using namespace std;
string a;
int i,j,lmax=0;
int b[50000];
int main()
{
    cin>>a;
    for (i=0;i<a.size();i++)
    {
        int d=0;
        memset(b,0,sizeof(b));
        for (j=i;j<a.size();j++)
        if (b[a[j]]==0)
        {
            d++;
            b[a[j]]++;
        }
        else break;
        lmax=max(lmax,d);
    }
    cout<<lmax;
}
    
